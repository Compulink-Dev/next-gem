{
    "version": 2,
    "public": false,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ],
    "rewrites": [
      {
        "source": "/:tenant((?!api|_next|static|favicon.ico).*)",
        "destination": "/[tenant]"
      }
    ]
  }